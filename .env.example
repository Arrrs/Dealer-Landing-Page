# Front-end / Build Environment Variables
# Copy this file to .env.local and fill in your actual values

# Google Apps Script Web App URL (get this after deploying your Apps Script)
NEXT_PUBLIC_SUBMIT_URL=https://script.google.com/macros/s/AKfycby9A1QL7NiCZn-p-5nkaYV5owINUyNf6Rng_RmYuwCEooUaUFoVTEDMFgQ9nXR9Mt-o/exec

# Secret token for form submission (must match SECRET in Google Apps Script)
NEXT_PUBLIC_SITE_TOKEN=your_secret_token_here

# App name
NEXT_PUBLIC_APP_NAME=LearnToDeal

# Optional: Analytics / tracking
# NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X

# Local dev only
NODE_ENV=development
